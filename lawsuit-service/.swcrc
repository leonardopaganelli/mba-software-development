{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@scripts/*": ["./scripts/*"],
      "@repositories/*": ["./src/repositories/*"],
      "@clients/*": ["./src/clients/*"]
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs"
  }
}
